<section class="container">
<h1><%= currentUser.username %> Profile</h1>
  <h4>description</h4>
  <p><%= user.description %></p>
  <h4>email</h4>
  <p> <%= user.email %></p>
  <h4>preferences</h4>
  <p> <%= user.preferences %></p>
  <% events.forEach((item)=>{ %>
     <%- include ../../partials/event.ejs %>
  <% }) %>
  <a class="button button-margin"href="/user/profile/me/edit">Edit</a>
</section>

