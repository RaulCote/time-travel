<section class="container">
  <h1>Delete Event</h1>
  <h3><%= event.name %></h3>
  <img class="event-image" src="<%= event.picture %>" alt="">
  <p>If you go forward, this event will be removed.</p>
  <form method="POST" action="/events/<%=event._id %>/delete">
    <button class="button button-margin">Delete</button>
  </form> 
</section>
